<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>items</key>
	<array>
		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>label</key>		<string>Instapaper</string>
			<key>footerText</key>	<string>Credentials are checked when saving a URL.</string>		
		</dict>

		<dict>
			<key>cell</key>			<string>PSEditTextCell</string>
			<key>id</key> 			<string>UsernameField</string>
			<key>defaults</key>		<string>libactivator.actapaper</string>
			<key>key</key>			<string>username</string>
			<key>detail</key>		<string>PSDetailController</string>
			<key>isEmail</key> 		<true/>
			<key>placeholder</key>	<string>email</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSSecureEditTextCell</string>
			<key>id</key> 			<string>PasswordField</string>
			<key>defaults</key>		<string>libactivator.actapaper</string>
			<key>key</key>			<string>password</string>
			<key>detail</key>		<string>PSDetailController</string>
			<key>placeholder</key>	<string>password</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>save credentials</string>
			<key>action</key>  		<string>resignKeyboard</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
		</dict>

		<dict>
			<key>cell</key> 			<string>PSLinkCell</string>
			<key>label</key> 			<string>Activation Methods</string>
			<key>isController</key> 	<true/>
			<key>bundle</key> 			<string>LibActivator</string>			
			<key>activatorListener</key><string>libactivator.actapaper</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>label</key>		<string>Contact</string>
			<key>footerText</key>	<string>Thanks for using Actapaper! You're running full release 1.0.1</string>
		</dict>

		<dict>
			<key>cell</key> 		<string>PSButtonCell</string>
			<key>label</key> 		<string>Support &amp; Feedback</string>
			<key>icon</key> 		<string>mail.png</string>
			<key>action</key> 		<string>mail</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>@insanj</string>
			<key>icon</key> 		<string>twitter.png</string>
			<key>action</key>  		<string>twitter</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSButtonCell</string>
			<key>label</key>		<string>Donate</string>
			<key>icon</key> 		<string>paypal.png</string>
			<key>action</key>  		<string>donate</string>
		</dict>

		<dict>
			<key>cell</key>			<string>PSGroupCell</string>
			<key>footerText</key>	<string>&#169; 2014 Julian Weiss&#xD;Sentry's Heart used with permission</string>
		</dict>
	</array>
	
	<key>title</key>	<string>Actapaper</string>
</dict>
</plist>
